<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sonkamble.app.car_services">
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/maintenance"
        android:label="@string/app_name"
        android:roundIcon="@drawable/maintenance"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="sonkamble.app.car_services.Login"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sonkamble.app.car_services.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="sonkamble.app.car_services.Add_Route"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="sonkamble.app.car_services.Add_Service_Center"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="sonkamble.app.car_services.Add_Area"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="sonkamble.app.car_services.Service_Center_List"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>